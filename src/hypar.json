{
  "description": "A function from testfit to generate simple buildings.",
  "function": "testfit-makebuilding::Hypar.Function::Handler",
  "function_id": "testfit-makebuilding",
  "name": "Make Building",
  "runtime": "dotnetcore2.0",
  "parameters": {
    "boundary": {
      "description": "Site boundary polygon",
      "type": "location"
    },
    "width": {
      "min": 10.0,
      "max": 40.0,
      "step": 1.0,
      "description": "Building mass width.",
      "type": "number"
    },
    "height": {
      "min": 4.0,
      "max": 64.0,
      "step": 1.0,
      "description": "Building mass height.",
      "type": "number"
    },
    "aspectRatio": {
      "min": 0.0,
      "max": 1.0,
      "step": 0.05,
      "description": "Maximum height/width aspect ratio for courtyards.",
      "type": "number"
    }
  },
  "respository_url": "https://github.com/rgriege/testfit-hypar",
  "returns": {
    "result": {
      "description": "The result",
      "type": "number"
    }
  },
  "version": "0.0.6"
}
